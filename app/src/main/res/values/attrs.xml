<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable
        name="Toggle">
        <attr
            name="textOn"
            format="string"/>

        <attr
            name="textOff"
            format="string"/>

        <attr
            name="colorOffs"
            format="color"/>

        <attr
            name="colorOns"
            format="color"/>

        <attr
            name="colorBorder"
            format="color"/>

        <attr
            name="colorTextOn"
            format="color"/>

        <attr
            name="colorTextOff"
            format="color"/>

        <attr
            name="colorDisabled"
            format="color"/>

        <attr name="on"
            format="boolean"/>

        <attr name="android:textSize"/>
        
        <attr name="android:enabled"/>
    </declare-styleable>
</resources>